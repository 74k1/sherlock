<interface>
    <object class="GtkBox" id="holder">
        <!-- Horizontal Split for Shortcuts -->
        <property name="orientation">horizontal</property>
        <property name="spacing">20</property>
        <property name="hexpand">true</property> 

        <child>
            <object class="GtkBox" id="app-name-holder">
                <property name="orientation">vertical</property>
                <property name="valign">center</property>
                <property name="hexpand">true</property>

                <child>
                    <object class="GtkBox" id="bulk-text-tilte-bar">
                        <property name="valign">center</property>
                        <property name="name">bulk-text-title-bar</property>
                        <property name="vexpand">false</property>
                        <child>
                            <object class="GtkImage" id="icon-name">
                                <property name="name">bulk-text-icon</property>
                                <property name="icon-name">teams</property>
                                <property name="pixel-size">13</property>   
                            </object>
                        </child>
                        <child>
                            <object class="GtkBox">
                                <property name="valign">center</property>
                                <child>
                                    <object class="GtkLabel" id="launcher-type">
                                        <property name="name">bulk-text-title</property>
                                        <property name="label">Teams Event</property>
                                        <property name="xalign">0</property>
                                    </object>
                                </child>
                            </object>
                        </child>
                    </object>
                </child>

                <child>
                    <object class="GtkBox" id="time-holder">
                        <property name="orientation">horizontal</property>
                        <child>
                            <object class="GtkLabel" id="time-label">
                                <property name="name">time-label</property>
                                <property name="xalign">0</property>
                                <property name="label"></property>
                                <property name="valign">end</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkLabel" id="end-time-label">
                                <property name="name">end-time-label</property>
                                <property name="xalign">0</property>
                                <property name="label">Test</property>
                                <property name="valign">end</property>
                                <property name="margin-bottom">5</property>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="title-label">
                        <property name="name">title-label</property>
                        <property name="xalign">0</property>
                        <property name="label"></property>
                    </object>
                </child>

            </object>
        </child>

        <child>
            <object class="GtkBox" id="shortcut-holder">
                <property name="valign">center</property>
                <property name="name">shortcut-holder</property>
                <property name="visible">false</property>
                <property name="orientation">horizontal</property>
                <property name="spacing">5</property>
                <child>
                    <object class="GtkLabel">
                        <property name="name">shortcut-modkey</property>
                        <property name="label"></property>   
                    </object>
                </child>
                <child>
                    <object class="GtkLabel">
                        <property name="name">shortcut</property>
                        <property name="label"></property>   
                    </object>
                </child>
            </object>
        </child>

    </object>
</interface>
